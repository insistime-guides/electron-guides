{
  "name": "electron-guides",
  "version": "0.2.2",
  "private": true,
  "description": "electron guides",
  "keywords": [
    "electron",
    "guides"
  ],
  "scripts": {
    "build-main": "cd electron-src/electron-app && npm i",
    "build-index": "cd electron-src/renderer-index && npm i && npm run build",
    "build-login": "cd electron-src/renderer-login && npm i && npm run build",
    "build-all": "npm run build-main && npm run build-index && npm run build-login",
    "start": "electron electron-src",
    "dist": "qelectron d ./electron-pack/dist.config.js",
    "prepackmac": "npm run init && npm run build-all && npm run dist",
    "packmac": "qelectron pm ./electron-pack/packmac.config.js",
    "packdmg": "qelectron pd ./electron-pack/packdmg.config.js"
  },
  "author": {
    "name": "qiaowenbin",
    "email": "npm@insistime.com",
    "url": "https://insistime.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insistime/electron-guides.git"
  },
  "devDependencies": {
    "electron": "^17.1.2",
    "qiao-electron-cli": "^0.0.8"
  },
  "dependencies": {
    "log4js": "^6.4.3"
  }
}
